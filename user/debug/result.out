FUNCTION_DECL int main
  COMPOUND_STMT
    DECL_STMT
      VAR_DECL a int
        INTEGER_LITERAL 0
      VAR_DECL b int
        INTEGER_LITERAL 1
    BINARY_OPERATOR =
      DECL_REF_EXPR a
      BINARY_OPERATOR +
        DECL_REF_EXPR a
        DECL_REF_EXPR b
    IF_STMT
      BINARY_OPERATOR ||
        DECL_REF_EXPR a
        DECL_REF_EXPR b
      BINARY_OPERATOR =
        DECL_REF_EXPR b
        BINARY_OPERATOR +
          DECL_REF_EXPR b
          INTEGER_LITERAL 1
    WHILE_STMT
      BINARY_OPERATOR +
        DECL_REF_EXPR a
        DECL_REF_EXPR b
      COMPOUND_STMT
        BINARY_OPERATOR =
          DECL_REF_EXPR b
          BINARY_OPERATOR +
            DECL_REF_EXPR b
            INTEGER_LITERAL 1
    RETURN_STMT
      INTEGER_LITERAL 0
